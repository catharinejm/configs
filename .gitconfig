[user]
  name = Jon Distad
  email = jon.distad@gmail.com
[color]
	branch = auto
	diff = auto
	status = auto
[color "branch"]
	current = yellow reverse
	local = yellow
	remote = green
[color "diff"]
	meta = yellow bold
	frag = magenta bold
	old = red bold
	new = green bold
[color "status"]
	added = yellow
	changed = green
	untracked = cyan
[merge]
	tool = opendiff
[core]
	legacyheaders = false
[repack]
	usedeltabaseoffset = true
[alias]
	ci = commit
	co = checkout
	st = status
  ch = cherry -v origin
