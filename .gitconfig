[user]
  name = Jon Distad
  email = jon@theedgecase.com
[color]
	branch = auto
	diff = auto
	status = auto
[color "branch"]
	current = yellow reverse
	local = yellow
	remote = green
[color "diff"]
	meta = yellow bold
	frag = magenta bold
	old = red bold
	new = green bold
[color "status"]
	added = yellow
	changed = green
	untracked = cyan
[merge]
	tool = opendiff
[core]
	legacyheaders = false
[repack]
	usedeltabaseoffset = true
[alias]
	ci = commit
	co = checkout
	st = status
  ch = cherry -v origin
[git-pair]
	authors = Jon Distad <jon@theedgecase.com>
	authors = Ken Barker <ken@theedgecase.com>
	authors = Felix Flores <felix@theedgecase.com>
